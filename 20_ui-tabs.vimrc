"nnoremap <C-PageUp> :tabnext<CR>
"inoremap <C-PageUp> <Esc>:tabnext<CR>
"vnoremap <C-PageUp> :<C-U>tabnext<CR>

"nnoremap <C-PageUp> :tabprev<CR>
"inoremap <C-PageUp> <Esc>:tabprev<CR>
"vnoremap <C-PageUp> :<C-U>tabprev<CR>

"nnoremap H gT
"nnoremap L gt

"inoremap <C-Left> <Esc>:tabprev<CR>
"inoremap <C-Right> <Esc>:tabnext<CR>
"vnoremap <C-Left> :<C-U>tabprev<CR>
"vnoremap <C-Right> :<C-U>tabnext<CR>

map <C-Left> gT
map <C-Right> gt
map! <C-Left> gT
map! <C-Right> gt
